<h1>Edit Your Place Details</h1>
<div class="booyah-box login-form col-xs-10 col-xs-offset-1">
  <%= simple_form_for @place do |f| %>
    <%= f.input :name %><br>
    <%= f.input :address %><br>
    <%= f.input :description %><br>
    <%= f.submit 'Submit Your Edited Listing', class: 'btn btn-primary' %>
  <% end %>
</div>